{
	"languageserver": {
		"golang": {
			"command": "go-langserver",
			"filetypes": ["go"],
			"initializationOptions": {
				"gocodeCompletionEnabled": true,
				"diagnosticsEnabled": true,
				"lintTool": "golint"
			}
		}
	},
	"diagnostic.displayByAle": true,
  "diagnostic.refreshOnInsertMode": true,
  "diagnostic.signOffset": 999999,
  "diagnostic.errorSign": "✘",
  "diagnostic.warningSign": "⚠",
  "diagnostic.infoSign": "",
  "diagnostic.hintSign": "ஐ",
  "diagnostic.refreshAfterSave": true,
  "diagnostic.checkCurrentLine": true,
  "diagnostic.virtualTextPrefix": " ❯❯❯ ",
  "diagnostic.virtualText": true,
  "diagnostic.locationlist": true,
  "diagnostic.enableMessage": "never",
  "suggest.noselect": true,
  "suggest.detailField": "abbr",
  "suggest.snippetIndicator": "",
  "suggest.removeDuplicateItems": true,
  "suggest.triggerAfterInsertEnter": true,
  "suggest.timeout": 5000,
  "suggest.minTriggerInputLength": 1,
  "elixirLS.filetypes": ["elixir", "eelixir"],
  "elixir.filetypes": ["elixir", "eelixir", "ex", "exs", "eex", "leex"]
}
